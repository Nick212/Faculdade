import java.util.Scanner;

public class Fila {

	public static void main(String[] args) {

	}

	/**
	 * 05) Criar um programa que leia pelo teclado um vetor de inteiros de 20
	 * posições, separe os elementos pares e ímpares em dois vetores, e
	 * exiba-os.
	 */
	public static boolean exercicio05() {
		
		try {
			showMessage("Digite um Número:");
			int[] listNumber = new int[20];
			
			return true;
		} catch (Exception e) {
			return false;
		}		
	}

	private static String readKeyBorad() {
		Scanner sc = new Scanner(System.in);
		return sc.nextLine();
	}
	
	private static void showMessage(String msg){
		System.out.println(msg);
	}

}
