import java.util.Scanner;


public class Exe08 {

	public static void main(String[] args) {
		int vetor[] = new int[9];

		Scanner sc = new Scanner(System.in);

		for (int i = 0; i < vetor.length; i++) {
			System.out.println("Digite um Número: ");
			int valueRead = sc.nextInt();
			if (valueRead % 2 == 0) {
				vetor[i] = valueRead ;
			}
		}
		System.out.println("Digite um Número: ");
		int valueRead = sc.nextInt();
		
		existValueInsideList(valueRead,  vetor);
	}
protected static boolean existValueInsideList(int value, int[] vetor){
	for (int i = 0; i < vetor.length; i++)
		vetor[i] == value ? return true
	
	return true;
}
}
